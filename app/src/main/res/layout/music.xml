<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:background="#e2dFd6"
    android:layout_height="match_parent"
   >

        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:background="@drawable/northeast1"
            android:textSize="20dp"
            android:textColor="#fff"
            android:id="@+id/t2"
            />
        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:background="@drawable/northeast"
            android:text="#东北歌#"
            android:textSize="20dp"
            android:textColor="#fff"
            android:id="@+id/t1"
            />
        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:layout_marginLeft="190dp"
            android:background="@drawable/minnan1"
            android:id="@+id/t4"
            />
        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:layout_marginLeft="190dp"
            android:background="@drawable/minnan"
            android:text="#闽南语#"
            android:textSize="20dp"
            android:textColor="#fff"
            android:id="@+id/t3"
            />


        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:background="@drawable/shanghai1"
            android:layout_below="@+id/t1"
            android:id="@+id/t6"
            />
        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:background="@drawable/shanghai"
            android:text="#上海歌#"
            android:textSize="20dp"
            android:textColor="#fff"
            android:layout_below="@+id/t1"
            android:id="@+id/t5"
            />
        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:layout_marginLeft="190dp"
            android:background="@drawable/yue1"
            android:layout_below="@+id/t3"
            android:id="@+id/t8"
            />
        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:layout_marginLeft="190dp"
            android:background="@drawable/yue"
            android:layout_below="@+id/t3"
            android:text="#粤语#"
            android:textSize="20dp"
            android:textColor="#fff"
            android:id="@+id/t7"
            />


        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:background="@drawable/hebei1"
            android:layout_below="@+id/t5"
            android:id="@+id/t10"
            />
        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:background="@drawable/hebei"
            android:text="#河北歌#"
            android:textSize="20dp"
            android:textColor="#fff"
            android:layout_below="@+id/t5"
            android:id="@+id/t9"
            />
        <Button
        android:layout_width="190dp"
        android:layout_height="171dp"
        android:layout_marginLeft="190dp"
        android:background="@drawable/changsha1"
        android:layout_below="@+id/t7"
        android:id="@+id/t12"
        />
        <Button
            android:layout_width="190dp"
            android:layout_height="171dp"
            android:layout_marginLeft="190dp"
            android:background="@drawable/changsha"
            android:layout_below="@+id/t7"
            android:text="#长沙语#"
            android:textSize="20dp"
            android:textColor="#fff"
            android:id="@+id/t11"
            />


</RelativeLayout>





